{
  "name": "contact-list-ts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.6.2",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "clsx": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "typescript": "~4.3.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .tsx",
    "lint:fix": "eslint --fix ."
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "keyword-spacing": [
        "error",
        {
          "before": true,
          "after": true
        }
      ],
      "no-extra-semi": "error",
      "semi": [
        "error",
        "always"
      ],
      "semi-style": [
        "error",
        "last"
      ],
      "space-before-blocks": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^6.2.1"
  },
  "engines": {
    "node": "16",
    "npm": "please-use-yarn",
    "yarn": "1"
  }
}